package vista;

import java.util.Scanner;
import model.Nota;

public class Lista2 {

	public static void main(String[] args) {
		
		Nota prima = new Nota();
		prima.descrizione = "pane";
		prima.completa();
		System.out.println(prima.stampa());
		
		Nota seconda = new Nota();
		seconda.descrizione = "latte";
		System.out.println(seconda.stampa());
	}

}
