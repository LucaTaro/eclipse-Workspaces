package vista;

import java.util.Scanner;
import model.Nota;

public class Lista2 {

	public static void main(String[] args) {
		
		Nota[] lista = new Nota[10];
		
		/*Nota prima = new Nota();
		prima.descrizione = "pane";
		prima.completa();
		System.out.println(prima.stampa());*/
		lista[0] = new Nota();
		lista[0].descrizione = "pane";
		lista[0].completa();
		System.out.println(lista[0].stampa());
		
		Nota seconda = new Nota();
		seconda.descrizione = "latte";
		System.out.println(seconda.stampa());
		
		for (int i = 0; i < lista.length; i++) {
			if (lista[i] != null) {
				System.out.println(lista[i].stampa());
			}
		}
	}

}
